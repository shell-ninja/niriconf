  "tokenColors": [
    {
      "name": "Base - Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#DD92F677"
      }
    },
    {
      "name": "Base - String",
      "scope": [
        "string.quoted punctuation",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#7EFDD0"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#C7C8FF",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Base - Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#69B4F9"
      }
    },
    {
      "name": "Base - Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C7C8FF"
      }
    },
    {
      "name": "Base - Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF8E8E"
      }
    },
    {
      "name": "Base - Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#DD92F6"
      }
    },
    {
      "name": "Base - Storage",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#DD92F6"
      }
    },
    {
      "name": "Base - Accessor",
      "scope": "keyword.operator.accessor",
      "settings": {
        "foreground": "#C7C8FF"
      }
    },
    {
      "name": "Base - Keywords (Control/Debugger/Constant)",
      "scope": [
        "support.constant",
        "keyword.other.special-method",
        "keyword.other.new",
        "keyword.other.debugger",
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#DD92F6"
      }
    },
    {
      "name": "Base - Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#7EFDD0"
      }
    },
    {
      "name": "Base - Escape Character",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#DD92F6"
      }
    },
    {
      "name": "Base - URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Base - Language Variable",
      "scope": [
        "variable.language"
